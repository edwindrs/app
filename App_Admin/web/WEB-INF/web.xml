<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>co.uniminuto.edu.servlets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>salir</servlet-name>
        <servlet-class>co.uniminuto.edu.servlets.salir</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>crearDeporte</servlet-name>
        <servlet-class>co.uniminuto.edu.servlets.crearDeporte</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>actualizaResultado</servlet-name>
        <servlet-class>co.uniminuto.edu.servlets.actualizaResultado</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>crearEquipo</servlet-name>
        <servlet-class>co.uniminuto.edu.servlets.crearEquipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>crearPartido</servlet-name>
        <servlet-class>co.uniminuto.edu.servlets.crearPartido</servlet-class>
    </servlet>
    <servlet>
        <description>Generated servlet for Java ME Client to Web Application </description>
        <display-name>Javon service for  co.uniminuto.edu.servlets.WebToMobileServlet</display-name>
        <servlet-name>WebToMobileServlet</servlet-name>
        <servlet-class>co.uniminuto.edu.servlets.WebToMobileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>salir</servlet-name>
        <url-pattern>/salir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crearDeporte</servlet-name>
        <url-pattern>/crearDeporte</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>actualizaResultado</servlet-name>
        <url-pattern>/actualizaResultado</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crearEquipo</servlet-name>
        <url-pattern>/crearEquipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crearPartido</servlet-name>
        <url-pattern>/crearPartido</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WebToMobileServlet</servlet-name>
        <url-pattern>/servlet/co.uniminuto.edu.servlets.WebToMobileServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>30</session-timeout>
    </session-config>
</web-app>
